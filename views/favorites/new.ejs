<div class="modal-header">
    <h5 class="modal-title">Add an Item</h5>
</div>
<div class="modal-body">
            <form action='/favorites' method='POST'>
                <div class='form-group'>
                    Website: <input class='form-control modal-input' type='string' name='website'>
                </div>               
                
                <div class='form-group'>
                    label: <input class='form-control modal-input' type='string' name='label'>
                </div>
                <div class='form-group'>
                    <button class="btn btn-lg btn-default btn-block btn-primary modal-btn">Submit</button>
                    <a href='/dashboard' class='btn btn-lg bt-warning btn-block modal-btn'>Go Back</a>
                </div>
            </form>
        </div>
</div>
